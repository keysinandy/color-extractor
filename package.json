{
  "name": "@key/color-extractor",
  "version": "0.1.0",
  "description": "Extract colors from an image",
  "author": {
    "name": "Key Sin",
    "email": "keysinandy@gmail.com"
  },
  "license": "ISC",
  "types": "index.d.ts",
  "main": "dist/index.js",
  "module": "es/index.mjs",
  "exports": {
    ".": {
      "import": "./es/index.mjs",
      "require": "./dist/index.js",
      "types": "./index.d.ts"
    },
    "./utils": {
      "import": "./es/utils.mjs",
      "require": "./dist/utils.js",
      "types": "./index.d.ts"
    }
  },
  "scripts": {
    "build": "rimraf dist && rimraf es && node_modules/.bin/rollup -c rollup.config.ts --configPlugin typescript",
    "build:watch": "rimraf dist && rimraf es && node_modules/.bin/rollup -c rollup.config.ts -w --configPlugin typescript",
    "build:publish": "npm run build && npm version patch && npm publish --registry https://registry.npmjs.org"
  },
  "keywords": [
    "extractor",
    "color",
    "palette",
    "image",
    "canvas"
  ],
  "devDependencies": {
    "@rollup/plugin-buble": "^1.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.1",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "rollup": "^3.11.0",
    "rollup-plugin-dts": "^6.0.2",
    "tslib": "^2.5.0",
    "typescript": "^4.9.4"
  }
}